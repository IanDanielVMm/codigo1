<button onclick="mostrarComentario()">Mostrar Comentario</button>
<p id="comentario"></p>

<script>
const comentarios = [
    "¡Gran sitio web!",
    "Muy informativo.",
    "Me encanta el diseño.",
    "Muy útil, gracias."
];

function mostrarComentario() {
    const comentario = comentarios[Math.floor(Math.random() * comentarios.length)];
    document.getElementById("comentario").innerText = comentario;
}
</script>


