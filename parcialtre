    <h1>Preferencias del Usuario</h1>
    <label for="backgroundColor">Color de fondo:</label>
    <input type="color" id="backgroundColor" onchange="setBackgroundColor()">
    <br><br>
    <label for="fontFamily">Fuente preferida:</label>
    <select id="fontFamily" onchange="setFontFamily()">
        <option value="Arial">Arial</option>
        <option value="Verdana">Verdana</option>
        <option value="Courier New">Courier New</option>
    </select>
    <br><br>
    <label for="theme">Tema:</label>
    <select id="theme" onchange="setTheme()">
        <option value="light">Claro</option>
        <option value="dark">Oscuro</option>
        <option value="colorful">Colorido</option>
    </select>
    <br><br>
    <label for="termsAccepted">Acepto los términos y condiciones:</label>
    <input type="checkbox" id="termsAccepted">
    <button onclick="setTermsAccepted()">Guardar Preferencias</button>
    <br><br>
    <p id="termsStatus"></p>
    <script>
        // Función para aplicar el color de fondo
        function applyBackgroundColor(color) {
            document.body.style.backgroundColor = color;
        }

        // Función para aplicar la fuente
        function applyFontFamily(font) {
            document.body.style.fontFamily = font;
        }

        // Función para aplicar el tema
        function applyTheme(theme) {
            if (theme === 'dark') {
                document.body.style.backgroundColor = '#333';
                document.body.style.color = '#fff';
            } else if (theme === 'colorful') {
                document.body.style.backgroundColor = 'pink';
                document.body.style.color = 'purple';
            } else {
                document.body.style.backgroundColor = '#fff';
                document.body.style.color = '#333';
            }
        }

        // Función para guardar el color de fondo
        function setBackgroundColor() {
            let color = document.getElementById('backgroundColor').value;
            setCookie('backgroundColor', color, 7);
            applyBackgroundColor(color);
        }

        // Función para guardar la fuente preferida
        function setFontFamily() {
            let font = document.getElementById('fontFamily').value;
            setCookie('fontFamily', font, 7);
            applyFontFamily(font);
        }

        // Función para guardar el tema seleccionado
        function setTheme() {
            let theme = document.getElementById('theme').value;
            setCookie('theme', theme, 7);
            applyTheme(theme);
        }

        // Función para aceptar cookies y ocultar el banner
        function acceptCookies() {
            setCookie("cookiesAccepted", "true", 7);
            document.getElementById('cookie-banner').style.display = 'none';
        }

        // Función para guardar la aceptación de términos y condiciones
        function setTermsAccepted() {
            let accepted = document.getElementById('termsAccepted').checked;
            setCookie('termsAccepted', accepted, 7);
            document.getElementById('termsStatus').textContent = accepted ? "Términos aceptados" : "Términos no aceptados";
        }

        // Función para establecer una cookie
        function setCookie(name, value, days) {
            let expires = "";
            if (days) {
                let date = new Date();
                date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
                expires = "; expires=" + date.toUTCString();
            }
            document.cookie = name + "=" + (value || "") + expires + "; path=/";
        }

        // Función para obtener una cookie
        function getCookie(name) {
            let nameEQ = name + "=";
            let ca = document.cookie.split(';');
            for (let i = 0; i < ca.length; i++) {
                let c = ca[i];
                while (c.charAt(0) == ' ') c = c.substring(1, c.length);
                if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length, c.length);
            }
            return null;
        }

        // Aplicar las preferencias al cargar la página
        window.onload = function() {
            if (document.cookie.indexOf('cookiesAccepted=true') !== -1) {
                document.getElementById('cookie-banner').style.display = 'none';
                applyBackgroundColor(getCookie('backgroundColor'));
                applyFontFamily(getCookie('fontFamily'));
                applyTheme(getCookie('theme'));
                let termsAccepted = getCookie('termsAccepted');
                if (termsAccepted !== null) {
                    document.getElementById('termsAccepted').checked = (termsAccepted === 'true');
                    document.getElementById('termsStatus').textContent = (termsAccepted === 'true') ? "Términos aceptados" : "Términos no aceptados";
                }
            }
        };
    </script>
